@charset "utf-8";
@import "commonstyle.css";
/* stylesheet for showing answers for examinations */

/* make sure solutions show */
  .solution { color: blue; background-color: white; }
  ins.solution { color: blue; text-decoration: underline;
                 font-size: inherit; line-height: inherit; }
  del.solution { color: blue; text-decoration: line-through;
                 font-size: inherit; line-height: inherit; }
